{
  "name": "feathers-solr",
  "description": "A service plugin for Solr",
  "version": "2.1.0",
  "homepage": "https://github.com/sajov/feathers-solr",
  "keywords": [
    "feathers",
    "feathers-plugin",
    "solr",
    "solr-client"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/sajov/feathers-solr/blob/master/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/sajov/feathers-solr.git"
  },
  "author": {
    "name": "Sascha Jovanoski"
  },
  "contributors": [],
  "bugs": {
    "url": "https://github.com/sajov/feathers-solr/issues"
  },
  "engines": {
    "node": ">= 6"
  },
  "main": "lib/",
  "types": "types",
  "scripts": {
    "publish": "NOgit push origin --tags && npm run changelog && git push origin",
    "changelog": "NOgithub_changelog_generator && git add CHANGELOG.md && git commit -am \"Updating changelog\"",
    "release:patch": "NOnpm version patch && npm publish",
    "release:minor": "NOnpm version minor && npm publish",
    "release:major": "NOnpm version major && npm publish",
    "lint": "semistandard --fix",
    "dtslint": "dtslint types",
    "mocha": "mocha --opts mocha.opts",
    "test": "npm run lint && npm run dtslint && npm run coverage",
    "example": "babel-node example/index",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha -- --opts mocha.opts"
  },
  "semistandard": {
    "env": [
      "mocha"
    ]
  },
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "@feathersjs/adapter-commons": "^4.3.8",
    "@feathersjs/commons": "^4.3.0",
    "@feathersjs/errors": "^4.3.4",
    "debug": "^4.1.1",
    "is-plain-object": "^3.0.0",
    "node-fetch": "^2.6.0",
    "qs": "^6.9.1"
  },
  "devDependencies": {
    "@feathersjs/adapter-tests": "^4.3.4",
    "@feathersjs/express": "^4.3.5",
    "@feathersjs/feathers": "^4.3.4",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "dtslint": "^2.0.2",
    "istanbul": "^1.1.0-alpha.1",
    "loud-rejection": "^2.2.0",
    "mocha": "^6.2.1",
    "semistandard": "^14.2.0"
  }
}
